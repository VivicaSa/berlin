{
    "name": "weebee/scraper",
    "description": "A web scraper for Culturious",
    "keywords": ["scraper", "slim", "tools"],
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^3.8",
        "slim/twig-view": "^2.2",
        "symfony/var-dumper": "^3.2",
        "vlucas/phpdotenv": "^2.4",
        "fabpot/goutte": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
